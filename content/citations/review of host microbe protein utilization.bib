@article{kangReprocessing16SRRNA2021,
  title = {Reprocessing {{16S rRNA Gene Amplicon Sequencing Studies}}: ({{Meta}}){{Data Issues}}, {{Robustness}}, and {{Reproducibility}}},
  shorttitle = {Reprocessing {{16S rRNA Gene Amplicon Sequencing Studies}}},
  author = {Kang, Xiongbin and Deng, Dong Mei and Crielaard, Wim and Brandt, Bernd W.},
  date = {2021-10-21},
  journaltitle = {Front Cell Infect Microbiol},
  volume = {11},
  eprint = {34746021},
  eprinttype = {pmid},
  pages = {720637},
  issn = {2235-2988},
  doi = {10.3389/fcimb.2021.720637},
  url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC8566820/},
  urldate = {2022-10-21},
  abstract = {High-throughput sequencing technology provides an efficient method for evaluating microbial ecology. Different bioinformatics pipelines can be used to convert 16S ribosomal RNA gene amplicon sequencing data into an operational taxonomic unit (OTU) table that is used to analyze microbial communities. It is important to assess the robustness of these pipelines, each with specific algorithms and/or parameters, and their influence on the outcome of statistical tests. Articles with publicly available datasets on the oral microbiome were searched for, and five datasets were retrieved. These were from studies on changes in microbiota related to smoking, oral cancer, caries, diabetes, or periodontitis. Next, the data was processed with four pipelines based on VSEARCH, USEARCH, mothur, and UNOISE3. OTU tables were rarefied, and differences in {$\alpha$}-diversity and {$\beta$}-diversity were tested for different groups in a dataset. Finally, these results were checked for consistency among these example pipelines. Of articles that deposited data, only 57\% made all sequencing and metadata available. When processing the datasets, issues were encountered, caused by read characteristics and differences between tools and their defaults in combination with a lack of detail in the methodology of the articles. In general, the four mainstream pipelines provided similar results, but importantly, P-values sometimes differed between pipelines beyond the significance threshold. Our results indicated that for published articles, the description of bioinformatics methods and data deposition should be improved, and regarding reproducibility, that analysis of multiple subsamples is required when using rarefying as library-size normalization method.},
  pmcid = {PMC8566820},
  keywords = {methods,toRead},
  file = {C\:\\Users\\bjl34716\\Zotero\\storage\\4FQ82BXQ\\Kang et al_2021_Reprocessing 16S rRNA Gene Amplicon Sequencing Studies.pdf}
}

@article{laccourreyeExplainableMachineLearning2022,
  title = {Explainable {{Machine Learning}} for {{Longitudinal Multi-Omic Microbiome}}},
  author = {Laccourreye, Paula and Bielza, Concha and Larra\~naga, Pedro},
  date = {2022-01},
  journaltitle = {Mathematics},
  volume = {10},
