  author = {Oakley, Brian B. and Lillehoj, Hyun S. and Kogut, Michael H. and Kim, Woo K. and Maurer, John J. and Pedroso, Adriana and Lee, Margie D. and Collett, Stephen R. and Johnson, Timothy J. and Cox, Nelson A.},
  date = {2014-11-01},
  journaltitle = {FEMS Microbiology Letters},
  volume = {360},
